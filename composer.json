{
    "name": "mohmann/array-transform",
    "description": "Transforms raw arrays from a given source mapping and back again",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Ohmann",
            "email": "martin@mohmann.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArrayTransform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArrayTransform\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "spatie/phpunit-watcher": "^1.5",
        "phpstan/phpstan": "^0.10.1",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpmetrics/phpmetrics": "^2.4",
        "phake/phake": "^3.1",
        "infection/infection": "^0.9.0"
    }
}
